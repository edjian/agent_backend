<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.aiforest.cloud.broker.admin.mapper.HousingMapper">

    <resultMap id="housingMap" type="com.aiforest.cloud.broker.common.entity.Housing">
        <id property="id" column="id"/>
        <result property="tenantId" column="tenant_id"/>
        <result property="serialNumber" column="serial_number"/>
        <result property="staging" column="staging"/>
        <result property="building" column="building"/>
        <result property="entrance" column="entrance"/>
        <result property="floor" column="floor"/>
        <result property="houseType" column="house_type"/>
        <result property="propertyType" column="property_type"/>
        <result property="floorage" column="floorage"/>
        <result property="insideFloorage" column="inside_floorage"/>
        <result property="unitPrice" column="unit_price"/>
        <result property="totalPrice" column="total_price"/>
        <result property="costPrice" column="cost_price"/>
        <result property="strikePrice" column="strike_price"/>
        <result property="contractPrice" column="contract_price"/>
        <result property="createTime" column="create_time"/>
        <result property="updateTime" column="update_time"/>
        <result property="createId" column="create_id"/>
        <result property="state" column="state"/>
    </resultMap>

    <sql id="housingSql">
        housing.`id`,
        housing.`tenant_id`,
        housing.`serial_number`,
        housing.`staging`,
        housing.`building`,
        housing.`entrance`,
        housing.`floor`,
        housing.`house_type`,
        housing.`property_type`,
        housing.`floorage`,
        housing.`inside_floorage`,
        housing.`unit_price`,
        housing.`total_price`,
        housing.`cost_price`,
        housing.`strike_price`,
        housing.`contract_price`,
        housing.`create_time`,
        housing.`update_time`,
        housing.`create_id`,
        housing.`state`
    </sql>

</mapper>
